# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# AI Services
ANTHROPIC_API_KEY=your_anthropic_api_key
LLM_MODEL=claude-3-7-sonnet-20250219

# Freesound API (Get free key at https://freesound.org/apiv2/apply)
FREESOUND_API_KEY=your_freesound_api_key

# Gateway Configuration
ADMIN_API_KEY=your-admin-secret-key
GATEWAY_HOST=0.0.0.0
GATEWAY_PORT=8000

# Redis (optional)
REDIS_URL=redis://redis:6379
USE_REDIS=false

# Security & Rate Limiting
MAX_API_EXECUTION_TIME=30
MAX_REQUEST_SIZE=10485760
RATE_LIMIT_FREE=100
RATE_LIMIT_PRO=1000
RATE_LIMIT_ENTERPRISE=10000

# Monitoring (optional - get free account at sentry.io)
SENTRY_DSN=
ENVIRONMENT=production

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
STRIPE_PRO_PRICE_ID=your_stripe_pro_price_id
STRIPE_ENTERPRISE_PRICE_ID=your_stripe_enterprise_price_id

# MinIO Object Storage
MINIO_ENDPOINT=minio.systemd.diskstation.me
MINIO_ACCESS_KEY=admin
MINIO_SECRET_KEY=your_minio_password
MINIO_PUBLIC_URL=https://minio.systemd.diskstation.me
MINIO_SECURE=true